   "source": [
    "a = [1,2,3,4,5,6] # [5,2,3,5,7,9,8]\n",
    "i = 2\n",
    "k = 4\n",
    "while i < k:\n",
    "    a[i], a[k] = a[k] , a[i]\n",
    "    k -= 1\n",
    "    i += 1\n",
    "print(a)  \n"
   ]
  }
